    function CalculatePercent(sectionNum) {

        var total_indicator = 0;
        var total_action_checked = 0;
        var total_strength_checked = 0;
        var total_na = 0;
        var percent_action = 0;
        var percent_strength = 0;
        var percent_na = 0;

        total_indicator = $('input[type=radio][name^=s' + sectionNum + ']').length / 20;


        $('input[type=radio][name^=s' + sectionNum + ']:checked').each(function () {

            if ($(this).val() == 1 || $(this).val() == 2 || $(this).val() == 3) {
                total_action_checked = total_action_checked + 1;
            }
            else {
                total_strength_checked = total_strength_checked + 1;
            }

        });

        total_action_checked = total_action_checked / 4;
        total_strength_checked = total_strength_checked / 4;
        total_na = total_indicator - total_action_checked - total_strength_checked;

        percent_action = Math.round(total_action_checked / total_indicator * 100) / 100;
        percent_strength = Math.round(total_strength_checked / total_indicator * 100) / 100;
        percent_na = 1 - percent_action - percent_strength;
        //percent_action = parseInt(total_action_checked * 100 / total_indicator);
        //percent_action = parseInt(total_strength_checked * 100 / total_indicator);



        percent_action = (percent_action * 100).toString();


        if (percent_action.indexOf(".") >= 0)
            percent_action = percent_action.substring(0, percent_action.indexOf("."));

        percent_strength = (percent_strength * 100).toString();


        if (percent_strength.indexOf(".") >= 0)
            percent_strength = percent_strength.substring(0, percent_strength.indexOf("."));




        percent_na = (percent_na * 100).toString();


        if (percent_na.indexOf(".") >= 0)
            percent_na = percent_na.substring(0, percent_na.indexOf("."));

        $('#section-' + sesectionNum + '-action-summary').html("You indicated that " + percent_action + "% (" + total_action_checked + " of " + total_indicator + ") indicators are areas of action");
        $('#section-' + sesectionNum + '-strength-summary').html("You indicated that " + percent_strength + "% (" + total_strength_checked + " of " + total_indicator + ") indicators are areas of strength");
        $('#section-' + sesectionNum + '-na-summary').html("You indicated that " + percent_na + "% (" + total_na + " of " + total_indicator + ") indicators are N/A");
    }
</script>


