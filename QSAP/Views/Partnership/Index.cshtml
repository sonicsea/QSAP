<script src="~/Scripts/jquery.confirmExit.js"></script>
<script type="text/javascript">
    $(document).ready(function () {

        $("div.question").each(function (i) {
            if ($(this).hasClass("current")) {
                $(this).show();
            } else {
                $(this).hide();
            }
        });

        $("#summary").hide();

        $("#btnNext").click(function () {

            $("div.question").each(function (i) {
                if ($(this).hasClass("current")) {

                    if (!$(this).attr('itemid')) {
                        $("#summary").show();
                        $("#questionnaire").hide();
                        return false;
                    }
                    else {
                        var sectionNum = $(this).attr('itemid').substring(1, 2);
                        switch (sectionNum) {
                            case "1":
                                $('#section-number').html("1 of 3: STATE AND LOCAL LEADERS");
                                break;
                            case "2":
                                $('#section-number').html("2 of 3: STATE LEVEL LEADERS ONLY");
                                break;
                            case "3":
                                $('#section-number').html("3 of 3: LOCAL LEVEL LEADERS ONLY");
                                break;
                        }
                        //$('#section-number').html($(this).attr('itemid').substring(1, 2));
                    }
                    $(this).hide();
                    $(this).removeClass("current");
                    $('#' + $(this).attr('itemid')).addClass("current");
                    $('#' + $(this).attr('itemid')).show();
                    return false;
                }
            });

        });

        $("#btnPrev").click(function () {


            $("div.question").each(function (i) {


                if ($(this).hasClass("current")) {
                    if (!$(this).attr('itemref')) {
                        return false;
                    }
                    else {
                        var sectionNum = $(this).attr('itemref').substring(1, 2);
                        switch (sectionNum) {
                            case "1":
                                $('#section-number').html("1 of 3: STATE AND LOCAL LEADERS");
                                break;
                            case "2":
                                $('#section-number').html("2 of 3: STATE LEVEL LEADERS ONLY");
                                break;
                            case "3":
                                $('#section-number').html("3 of 3: LOCAL LEVEL LEADERS ONLY");
                                break;
                        }
                        //$('#section-number').html($(this).attr('itemref').substring(1, 2));
                    }

                    $(this).hide();
                    $(this).removeClass("current");
                    $('#' + $(this).attr('itemref')).addClass("current");
                    $('#' + $(this).attr('itemref')).show();
                    return false;
                }
            });

        });


        $('input[type=radio][name^=s1]').each(function () {

            if ($(this).attr('id').indexOf("_na") >= 0) {
                $(this).parent().parent().parent().parent().show();
            }
            else if ($(this).attr('id').indexOf("_view") >= 0 || $(this).attr('id').indexOf("_strength") >= 0) {
